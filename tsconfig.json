{
  "compilerOptions": {
    "allowImportingTsExtensions": true,
    "composite": true,
    "declaration": true,
    "incremental": true,
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "noEmit": true,
    "noImplicitAny": true,
    "noPropertyAccessFromIndexSignature": true,
    "noUncheckedIndexedAccess": true,
    "outDir": "dist",
    "skipDefaultLibCheck": true,
    "strict": true,
    "target": "ESNext",
    "verbatimModuleSyntax": true
  }
}
